                "summary": "Muestra usuarios y url para transacciones",
                "operationId": "be60d0fef1f88890e7645a0bf4f8be1b",
                "parameters": [
                    {
                        "name": "token",
                        "in": "path",
                        "description": "Parameter",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Muestra todos los usuarios de manera descendente, varias urls para visualizar las transacciones y paginación."
                    },
                    "401": {
                        "description": "Token incorrecto."
                    }
                }
            }
        },
        "/api/users/{token}/search/{val_search}": {
            "post": {
                "tags": [
                    "USERS"
                ],
                "summary": "Buscar a usuarios por su ID, Nombre o Email",
                "operationId": "5045d6ae092acc09cf58d2f2a0fd41ba",
                "parameters": [
                    {
                        "name": "token",
                        "in": "path",
                        "description": "Parameter",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "val_search",
                        "in": "path",
                        "description": "Parameter",
                        "required": true,
                        "schema": {
                            "type": "string"
